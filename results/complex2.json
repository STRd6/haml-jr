[
  {
    "text": "!!!\n"
  },
  {
    "tag": "html",
    "children": [
      {
        "tag": "head",
        "children": [
          {
            "tag": "title",
            "text": "Ravel | #{@name}'s photo tagged #{@tag}\n"
          },
          {
            "unbufferedCode": "@props.each (key, value) ->",
            "children": [
              {
                "tag": "meta",
                "attributes": [
                  {
                    "name": "property",
                    "value": "key"
                  },
                  {
                    "name": "content",
                    "value": "value"
                  }
                ]
              }
            ]
          },
          {
            "tag": "link",
            "attributes": [
              {
                "name": "href",
                "value": "\"/images/favicon.ico\""
              },
              {
                "name": "rel",
                "value": "\"icon\""
              },
              {
                "name": "type",
                "value": "\"image/x-icon\""
              }
            ]
          },
          {
            "tag": "link",
            "attributes": [
              {
                "name": "rel",
                "value": "\"stylesheet\""
              },
              {
                "name": "href",
                "value": "\"/stylesheets/normalize.css\""
              }
            ]
          },
          {
            "tag": "link",
            "attributes": [
              {
                "name": "rel",
                "value": "\"stylesheet\""
              },
              {
                "name": "href",
                "value": "\"/stylesheets/bootstrap.min.css\""
              }
            ]
          },
          {
            "tag": "link",
            "attributes": [
              {
                "name": "rel",
                "value": "\"stylesheet\""
              },
              {
                "name": "href",
                "value": "\"/stylesheets/main.css\""
              }
            ]
          },
          {
            "tag": "script",
            "attributes": [
              {
                "name": "src",
                "value": "\"//use.typekit.net/ghp4eka.js\""
              }
            ]
          },
          {
            "filter": "javascript",
            "content": "try{Typekit.load();}catch(e){}\n\n"
          }
        ]
      },
      {
        "tag": "body",
        "children": [
          {
            "classes": [
              "facebook"
            ],
            "tag": "div",
            "children": [
              {
                "tag": "header",
                "children": [
